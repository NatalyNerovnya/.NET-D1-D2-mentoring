<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="CustomSection" type="FileWatcherConsole.Configuration.CustomConfigSection, FileWatcherConsole"/>
  </configSections>

  <CustomSection>
    <Rules>
      <add nameTemplate="\w*test(\w*)" path="c:\task_bcl\tests" dateFormat="MM-dd-yy"/>
      <add nameTemplate="cat(\w*)" path="c:\task_bcl\cats" dateFormat="dd-mm-yyyy"/>
    </Rules>

    <Folder path="c:\bcl_folder" />
    <DefaultFolder path="c:\task_bcl\defaultFolder"/>
    <Culture value="ru-RU" />
  </CustomSection>


  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>


</configuration>